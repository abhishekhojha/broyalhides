# Dependencies
# ----------------------------------------------------------------
# Lockfiles are generally committed, but node_modules is never.
/node_modules


# Next.js
# ----------------------------------------------------------------
# Build outputs and cache
/.next/
/out/
/.swc/

# Static exports (if using `next export`)
/out/


# Environment Variables
# ----------------------------------------------------------------
# Never commit environment variables or secrets!
# Your team should use an ".env.example" file instead.
.env
.env.local
.env.development.local
.env.production.local
.env.test.local


# Logs
# ----------------------------------------------------------------
npm-debug.log*
yarn-debug.log*
yarn-error.log*


# IDE & Editor Configs
# ----------------------------------------------------------------
# VSCode
.vscode/
# Don't ignore user-specific settings
!.vscode/settings.json
!.vscode/extensions.json
!.vscode/launch.json
# JetBrains (WebStorm, etc.)
.idea/


# Operating System
# ----------------------------------------------------------------
# macOS
.DS_Store
# Windows
Thumbs.db


# Testing
# ----------------------------------------------------------------
/coverage


# Other
# ----------------------------------------------------------------
# Vercel serverless functions (if deploying to Vercel)
.vercel/